{
  "name": "choice-story-web",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "20"
  },
  "scripts": {
    "dev": "NODE_OPTIONS='--no-warnings' next dev",
    "devprod": "NODE_ENV=production NODE_OPTIONS='--no-warnings' next dev",
    "build": "NODE_OPTIONS='--no-warnings' next build",
    "start": "NODE_OPTIONS='--no-warnings' next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "ci:check": "npm run lint && npx tsc --noEmit && npm run build",
    "prod:build": "next build",
    "prod:start": "next start",
    "prod": "npm run prod:build && npm run prod:start",
    "deploy:prod": "firebase use choicestory-b3135 && npm run build && firebase deploy",
    "deploy:prod:hosting": "firebase use choicestory-b3135 && npm run build && firebase deploy --only hosting",
    "deploy:prod:functions": "firebase use choicestory-b3135 && cd functions && npm run build && cd .. && firebase deploy --only functions",
    "verify:firebase": "node firebase-config-verification.js"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@paypal/react-paypal-js": "^8.8.2",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.6",
    "@radix-ui/react-toggle": "^1.1.0",
    "@radix-ui/react-toggle-group": "^1.1.0",
    "@sentry/nextjs": "^10.0.0",
    "@types/uuid": "^10.0.0",
    "axios": "^1.7.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "firebase": "11.0.2",
    "firebase-admin": "13.0.1",
    "firebase-functions": "^6.3.2",
    "framer-motion": "^12.4.7",
    "i18next": "^25.2.1",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-resources-to-backend": "^1.2.1",
    "lucide-react": "^0.461.0",
    "next": "^15.1.7",
    "openai": "^5.6.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-i18next": "^15.5.3",
    "react-intersection-observer": "^9.16.0",
    "replicate": "^1.0.1",
    "sharp": "^0.33.5",
    "swagger-ui-dist": "^5.22.0",
    "swagger-ui-react": "^5.22.0",
    "tailwind-merge": "^2.6.0",
    "uuid": "^11.1.0",
    "zod": "^3.24.1",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@shadcn/ui": "^0.0.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/node-fetch": "^2.6.12",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.20",
    "eslint": "^8",
    "eslint-config-next": "15.0.3",
    "jest": "^29.7.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.15",
    "tailwindcss-animate": "^1.0.7",
    "ts-jest": "^29.3.1",
    "typescript": "^5"
  }
}
